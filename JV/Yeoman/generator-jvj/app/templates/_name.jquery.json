{
  "name": "<%= slugname %>",
  "title": "<%= props.title %>",
  "description": "<%= props.description %>",
  "version": "<%= props.version %>",
  "homepage": "<%= props.repository %>",
  "author": {
    "name": "<%= props.author_name %>",
    "url": "https://github.com/<%= props.github_username %>"
  },
  "repository": {
    "type": "git",
    "url": "<%= props.repository %>"
  },
  "bugs": "<%= props.bugs %>",
  "licenses": [
    {
      "type": "<%= props.license %>",
      "url": "http://opensource.org/licenses/<%= props.license %>"
    }
  ],
  "dependencies": {
    "jquery": "<%= props.jquery_version ? '~' + props.jquery_version : 'latest' %>"
  },
  "keywords": [
    "jquery-plugin"
  ]
}
